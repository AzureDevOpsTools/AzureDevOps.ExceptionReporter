<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <startup>
    <supportedRuntime sku=".NETFramework,Version=v4.0" version="v4.0" />
  </startup>

  <appSettings>
    <add key="pollintervall" value="60"/>
    <add key="serviceURL" value="http://localhost:2119/service.asmx"/>
    <add key="username" value="ValueSetATInstallTime"/>
    <add key="password" value="ValueSetAtInstallTime" />
    <add key="domain" value="ValueSetAtInstallTime" />
    <add key="httpSecurityMode" value="TransportCredentialOnly"/>
    <add key="clientCredentialType" value="Basic" />
    <add key="SendMailNotificationOnParseFailure" value="false"/>
    <add key="mailServerTo" value="ValueSetAtInstallTime"/>
    <add key="mailServerHost" value="ValueSetAtInstallTime"/>
    <add key="mailServerPort" value="25"/>
    <add key="mailServerLogin" value="ValueSetAtInstallTime"/>
    <add key="mailServerPassword" value="ValueSetAtInstallTime"/>
  </appSettings>
  
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="FailedExceptions.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <appender-ref ref="EventLogAppender"/>
      <level value="INFO" />
      <!--CHANGE TO WARN or ERROR if you want less logging-->
    </root>
  </log4net>

</configuration>