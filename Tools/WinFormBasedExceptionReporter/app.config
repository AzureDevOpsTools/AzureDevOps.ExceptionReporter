<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <add key="serviceURL" value="http://localhost:9072/service.asmx"/>
    <!--add key="serviceURL" value="http://exceptions.osiris.no/Service.asmx"/-->    
    <add key="ExceptionReporterUseGUI" value="true"/>
